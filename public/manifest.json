{
  "name": "eigen-plugin",
  "version": "1.0",
  "description": "eigen-plugin",
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "manifest_version": 3,
  "background": {
    "service_worker": "static/js/background.js",
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "static/css/content.css"
      ],
      "js": [
        "static/js/content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "declarativeContent",
    "storage",
  ],
  "web_accessible_resources": [
    {"resource": ["/images/app.png"], "matches": ["<all_urls>"]},
    {"resource": ["insert.js"], "matches": ["<all_urls>"]},
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "/images/app.png",
      "32": "/images/app.png",
      "48": "/images/app.png",
      "128": "/images/app.png",
    },
    "icons": {
      "16": "/images/app.png",
      "32": "/images/app.png",
      "48": "/images/app.png",
      "128": "/images/app.png",
    }
  },
}
